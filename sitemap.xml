---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>http://funkatron.com/</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>http://funkatron.com/about.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>http://funkatron.com/contact.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>http://funkatron.com/items.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>http://funkatron.com/posts.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.7</priority>
  </url>
  <url>
    <loc>http://funkatron.com/categories.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema}}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  {% for post in site.posts %}
  <url>
    <loc>http://funkatron.com{{ post.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  {% endfor %}
</urlset>